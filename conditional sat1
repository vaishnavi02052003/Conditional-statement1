#include<stdio.h>
int main ()
  {
	float s1,s2,s3,s4,s5,per;
	printf("entre the marks of s1 :");
	scanf("%f",&s1);
		printf("entre the marks of s2 :");
	scanf("%f",&s2);
		printf("entre the marks of s3 :");
	scanf("%f",&s3);
		printf("entre the marks of s4 :");
	scanf("%f",&s4);
		printf("entre the marks of s5 :");
	scanf("%f",&s5);
	
	per = (s1+s2+s3+s4+s5)/5.0;
	
		 if ( per>=85)
	{
		printf("gradeA\n");
	}
else
{
	if(per>=70)
	{
	 	printf("GradeB\n");
   	}
   	else
   	{
   		if (per>=55)
   		{
   			printf("GradeC\n");
		}
		else
		{
			if(per>=40)
			{
				printf("GradeD\n");
			}
		else
		{
			if (per<40)
			{
				printf("GradeF");
			}	
		}
		}
	   }
}		
}
